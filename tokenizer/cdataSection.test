            "description":"CDATA without end marker",
            "input":"foo",
            "initialStates":["CDATA section state"],
            "output":[["Character", "foo"]],
            "errors":[
                { "code": "eof-in-cdata", "line": 1, "col": 4 }
            ]
        },
        {
            "description":"CDATA with single bracket ending",
            "input":"foo]",
            "initialStates":["CDATA section state"],
            "output":[["Character", "foo]"]],
            "errors":[
                { "code": "eof-in-cdata", "line": 1, "col": 5 }
            ]
        },
