            "output":[["Character", "foo&#32;"]]
        },
        {
            "description":"CDATA followed by HTML content",
            "input":"foo&#32;]]>&#32;",
            "initialStates":["CDATA section state"],
            "output":[["Character", "foo&#32; "]]
        },
        {
            "description":"CDATA with extra bracket",
            "input":"foo]]]>",
            "initialStates":["CDATA section state"],
            "output":[["Character", "foo]"]]
        },
